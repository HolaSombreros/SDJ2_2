<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="500.0" spacing="10.0" userData="Login" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.LoginViewController">
   <children>
      <TextField fx:id="usernameInput" maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="300.0" promptText="Username">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button mnemonicParsing="false" onAction="#connect" prefHeight="50.0" prefWidth="300.0" text="Connect">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</VBox>
